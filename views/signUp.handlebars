
{{!-- sign up card --}}
<div class="container" style="position: absolute; top: 5%; margin-left: auto; margin-right: auto; left: 0; right: 0; z-index: 1;">
    <div class="row">
        <div class="col s12 m6 l4 xl4">
            <div class="card orange lighten-2">
                <div class="card-content white-text">
                    <span class="card-title">Welcome to Pixel+</span>
                    <p class="white-text">Track all the aspects of your lifestyle in one place. See how they come together to affect your mood.</p>
                </div>

                <div class="card-content white-text">
                    <span class="card-title">Create your account</span>
                </div>
                <form action="" method="POST">
                    <div class="card-action">
                        <label for="firstName" class="white-text">First Name</label>
                        <input class="white-text" type="text" name="firstName">

                        <label for="lastName" class="white-text">Last Name</label>
                        <input class="white-text" type="text" name="lastName">

                        <label for="email" class="white-text">Email</label>
                        <input class="white-text" type="email" name="email">

                        <label for="password1" class="white-text">Password</label>
                        <input class="white-text" type="password" maxlength="20" name="password1">

                        <label for="password2" class="white-text">Re-enter your password</label>
                        <input class="white-text" type="password" maxlength="20" name="password2">
                        <br>
                        <button class="btn waves-effect waves-light indigo lighten-1" type="submit" name="action">
                            Create Your Account <i class="material-icons">add</i>
                        </button>
                    </div>
                </form>

            </div>
        </div>
    </div>
</div>

